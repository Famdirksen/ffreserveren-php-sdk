{
    "name": "famdirksen/ffreserveren-php-sdk",
    "description": "An SDK to easily work with the FFReserveren API",
    "keywords": [
        "ffreserveren",
        "api",
        "sdk"
    ],
    "homepage": "https://github.com/famdirksen/ffreserveren-php-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Famdirksen",
            "email": "info@famdirksen.nl",
            "homepage": "https://famdirksen.nl",
            "role": "Company"
        },
        {
            "name": "Robin Dirksen",
            "email": "robin@famdirksen.nl",
            "homepage": "https://robindirksen.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "nesbot/carbon": "^2.35.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "vlucas/phpdotenv": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "OhDear\\PhpSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Famdirksen\\FFReserverenPhpSdk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
